{
    "alias": "keycloak-medelexis",
    "displayName": "Medelexis",
    "providerId": "keycloak-oidc",
    "enabled": false,
    "updateProfileFirstLoginMode": "on",
    "trustEmail": true,
    "storeToken": false,
    "addReadTokenRoleOnCreate": false,
    "authenticateByDefault": false,
    "linkOnly": false,
    "hideOnLogin": true,
    "config": {
      "tokenUrl": "https://keycloak.medelexis.ch/realms/Medelexis/protocol/openid-connect/token",
      "acceptsPromptNoneForwardFromClient": "false",
      "jwksUrl": "https://keycloak.medelexis.ch/realms/Medelexis/protocol/openid-connect/certs",
      "isAccessTokenJWT": "true",
      "claimFilterValue": ".*medelexis-ee-remote-realm-user.*",
      "filteredByClaim": "true",
      "backchannelSupported": "false",
      "caseSensitiveOriginalUsername": "false",
      "issuer": "https://keycloak.medelexis.ch/realms/Medelexis",
      "pkceMethod": "plain",
      "loginHint": "false",
      "clientAuthMethod": "client_secret_post",
      "syncMode": "FORCE",
      "clientSecret": "",
      "allowedClockSkew": "0",
      "userInfoUrl": "https://keycloak.medelexis.ch/realms/Medelexis/protocol/openid-connect/userinfo",
      "validateSignature": "true",
      "clientId": "ee-idp-$(env:EE_HOSTNAME_SHORT)",
      "uiLocales": "false",
      "disableNonce": "false",
      "useJwksUrl": "true",
      "sendClientIdOnLogout": "false",
      "pkceEnabled": "true",
      "metadataDescriptorUrl": "https://keycloak.medelexis.ch/realms/Medelexis/.well-known/openid-configuration",
      "authorizationUrl": "https://keycloak.medelexis.ch/realms/Medelexis/protocol/openid-connect/auth",
      "disableUserInfo": "false",
      "claimFilterName": "roles",
      "logoutUrl": "https://keycloak.medelexis.ch/realms/Medelexis/protocol/openid-connect/logout",
      "sendIdTokenOnLogout": "true",
      "passMaxAge": "false"
    }
  }